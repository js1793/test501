<p id="notice"><%= notice %></p>

<h1>Listing Emails</h1>

<table>
  <thead>
    <tr>
      <th>Appleader</th>
      <th>Email account</th>
      <th>Which phone</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @emails.each do |email| %>
      <tr>
        <td><%= email.appleader_id %></td>
        <td><%= email.email_account %></td>
        <td><%= email.which_phone %></td>
        <td><%= email.comments %></td>
        <td><%= link_to 'Show', email %></td>
        <td><%= link_to 'Edit', edit_email_path(email) %></td>
        <td><%= link_to 'Destroy', email, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Email', new_email_path %>
