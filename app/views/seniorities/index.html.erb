<p id="notice"><%= notice %></p>

<h1>Listing Seniorities</h1>

<table>
  <thead>
    <tr>
      <th>.gitignorecoach role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seniorities.each do |seniority| %>
      <tr>
        <td><%= seniority..gitignorecoach_role %></td>
        <td><%= link_to 'Show', seniority %></td>
        <td><%= link_to 'Edit', edit_seniority_path(seniority) %></td>
        <td><%= link_to 'Destroy', seniority, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seniority', new_seniority_path %>
